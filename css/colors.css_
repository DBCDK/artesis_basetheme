}
*/

/* extended search, 'sort by' & 'records shown' */


#edit-advanced .fieldset-wrapper {
  border-color: #e94928;
}


/* ***** Popup box ***** */


.ui-dialog {
    border: 2px solid #E94928;
    color: #2e2e2e;
    background-color: #ffffff;
}

.ui-dialog .ui-widget-header {
    color: #2e2e2e;
    background-color: #ffffff;
}

.ui-dialog .ui-widget-content {
    background-color: #ffffff;
}

.ui-dialog .ui-widget-content .status {
    color: #2e2e2e;
    background-color: #ffffff;
}

.ui-dialog .ui-widget-content .error {
    background-color: #ffffff;
}


/* ***** Autocomplete ***** */

#autocomplete {
    border-color: #E94928;
    background-color: #ffffff;
}

#autocomplete li.selected {
    color: #e94927;
    background-color: #ffffff;
}

#autocomplete li div.autocomplete_delimiter {
    border-color: #E94928;
    color: #2e2e2e;
    background-color: #ffffff;
}

/* *****  view-ding-event teaser list ****** */

.ding-event-teaser-list {
  background-color: #F2F2F2;
}

.ding-event-teaser-list h2 {
  color: #ffffff;
  background-color: #e94928;
}

/* *****  view-ding-event ****** */

.field-name-ding-news-library a,
.view-ding-news .ding-news-library a,
.view-ding-event .ding-event-library a,
.view-ding-event .views-field-group-audience a {
  color: #ffffff;
}

.view-ding-news .more-link a,
.view-ding-event .more-link a {
  color: #5C5959;
}

.view-ding-news .more-link a {
  background: url('../images/icons/icon_rightarrow_14x14_white_on_orange.png') no-repeat 0 60%;
}

.view-ding-event .more-link a {
  background: url('../images/icons/icon_rightarrow_14x14_white_on_orange_on_grey.png') no-repeat 0 60%;
}

.view-ding-news .more-link a:hover,
.view-ding-event .more-link a:hover {
  color: #e94927;
}


.ding-news-timestamp,
.view-ding-news .views-field-timestamp {
  color: #5C5959;
}

/* ***** Library menu block ***** */

#header .library-menu {
  border-top: 1px solid #AF3017;
  background-color: #E94928;
}

#header .library-menu .wrapper {
  border-top: 1px solid #D43C21;
}

#header .library-menu h2 {
  color: #ffffff;
  border-right: 1px solid #AF3017;
}

#header .library-menu .links .wrapper {
  border-left: 1px solid #D43C21;
}

#header .library-menu ul.links {
  background: transparent;
}

#header .library-menu ul.links li {
  border-right: 1px solid #AF3017;
}

#header .library-menu ul.links li a {
  color: #ffffff;
}

#header .library-menu ul.links a.active-trail,
#header .library-menu ul.links a.active {
  background: transparent;
  background-color: transparent;
  border-bottom: 3px solid #ffffff;
}

#header .library-menu ul.links li a:hover {
  color: #F49D91;
  background: transparent;
}

.today .ding-event-date p.ding-event-day,
.today .ding-event-date p.ding-event-month {
  color: #e94927;
}

#ding-library-page .library-list .middle-col.address a {
  background: url("../images/icons/icon_uparrow_14x14_white_on_orange.png") no-repeat 0 60%;
  color: #444;
}


/*
 * Ting carusel
 */

/* .ting-search-carousel ul.search-controller -> see also: Primary tabs */

.ting-search-carousel .ting-search-results {
    border: 1px solid #e94926;
}

.ting-search-carousel .ting-search-results .subtitle {
    color: #e94926;
}

.ting-search-carousel .rs-carousel-action-prev {
    background: url("../images/carousel_prev_40x70_orange_on_white.png");
}

.ting-search-carousel .rs-carousel-action-next {
    background: url("../images/carousel_next_40x70_orange_on_white.png");
}

.ting-search-carousel .rs-carousel-action-prev.rs-carousel-action-disabled {
    background-image: url("../images/carousel_prev_disabled_40x70_orange_on_white.png");
}

.ting-search-carousel .rs-carousel-action-next.rs-carousel-action-disabled {
    background-image: url("../images/carousel_next_disabled_40x70_orange_on_white.png");
}

